<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>cloud_service</servlet-name>
    <servlet-class>com.spark.CloudService</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>cloud_service</servlet-name>
    <url-pattern>/cloud_service</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>RobotCloud</servlet-name>
    <servlet-class>com.spark.RobotCloud</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RobotCloud</servlet-name>
    <url-pattern>/RobotCloud</url-pattern>
  </servlet-mapping>
</web-app>


